pipeline { 
	agent any
		stages {
			stage("build"){
				steps{
 					echo "build stage"
				     }
				       }
			stage("test"){
				steps{
					echo "test stage"
				     }
				       }
			stage("deploy"){
				steps{
					echo " deploy stage"
				     }
				       }
				       post{
				       success{
				       echo "build success"
				       }
				       failure{
				       echo "build failure"
				       }
				       }
			}
	}

